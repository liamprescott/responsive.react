  "scripts": {
    "dev.compile": "npm run dev.compile.app",
    "dev.compile.app": "set BUILD_DEV=1 && set BUILD_PRERELEASE=1 && set BUILD_RELEASE=0 && webpack",
    "dev.start": "npm-run-all -p dev.watch.app",
    "dev.watch.app": "nodemon -e js,jsx -w script/library -x \"npm run dev.compile.app\"",
    "release.clean": "rimraf ../Release/**/*",
    "release.clean.jsmaps": "rimraf ../Release/script/deploy/**.map",
    "release.compile": "npm run release.compile.app",
    "release.compile.app": "set BUILD_DEV=0 && set BUILD_PRERELEASE=0 && set BUILD_RELEASE=1 && webpack --config webpack.config.release.js",
    "release.gather": "npm run release.gather.fonts & npm run release.gather.images & npm run release.gather.html & npm run release.gather.webconfig",
    "release.gather.fonts": "copyfiles ./fonts/**/*.* ../Release",
    "release.gather.images": "copyfiles ./img/**/*.* ../Release",
    "release.gather.html": "copyfiles ./*.{htm,html} ../Release",
    "release.gather.webconfig": "copyfiles ./*.config ../Release",
    "release.publish": "npm run release.publish.ui",
    "release.publish.ui": "npm run release.clean && npm run release.gather && npm run release.compile && npm run release.clean.jsmaps"
  }
}
